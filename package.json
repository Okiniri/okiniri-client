  "private": false,
  "description": "",
  "main": "dist/node/index.js",
  "browser": "dist/browser/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "build": "npm run build:dev",
    "build:dev": "npx webpack --config webpack.config.js",
