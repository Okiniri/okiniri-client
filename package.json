{
  "name": "@okiniri/sdk",
  "private": false,
  "version": "0.0.0",
  "description": "SDK for the Okiniri API.",
  "author": "pldespaigne",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Okiniri/okiniri-sdk"
  },
  "main": "./dist/node/index.js",
  "browser": "./dist/browser/index.js",
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "start": "static . --port 5000",
    "dev": "npx webpack --config webpack.config.js",
    "build:prod": "npm run lint && npm run build:prod:node && npm run build:prod:browser",
    "build:prod:node": "npx tsc -p tsconfig.json",
    "build:prod:browser": "npx webpack --config webpack.config.production.js",
    "prepublishOnly": "npm run build:prod",
    "lint": "npx eslint ./src/**",
    "test": "jest --onlyChanged --runInBand",
    "test:full": "jest --runInBand"
  },
  "devDependencies": {
    "@jest/globals": "^26.6.2",
    "@types/jsrsasign": "^8.0.9",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "eslint": "^7.16.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^26.6.3",
    "node-static": "^0.7.11",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.12",
    "typescript": "^4.1.3",
    "webpack": "^5.11.0",
    "webpack-cli": "^4.2.0"
  },
  "dependencies": {
    "@okiniri/validate": "^1.0.0",
    "ajv": "^7.0.2",
    "cross-fetch": "^3.0.6",
    "jsrsasign": "^10.1.4"
  }
}
