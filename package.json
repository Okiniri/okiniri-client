    "eslint": "^7.13.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-import": "^2.22.1",
    "node-static": "^0.7.11",
    "ts-loader": "^6.1.2",
    "typescript": "^3.9.7",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9"
  },
  "dependencies": {
    "cross-fetch": "^3.0.6"
